%description:
Negative test case for assignment with units in NED.

%file: test.ned

// unit cases: not provided, SI, non-SI, unknown
simple Test
{
    parameters:
        @isNetwork(true);
        double p1 @unit(w) = 1s; // non convertible units
}

%file: test.cc

#include <omnetpp.h>

using namespace omnetpp;

namespace @TESTNAME@ {

class Test : public cSimpleModule
{
  public:
    Test() : cSimpleModule(16384) { }
    virtual void activity() override;
};

Define_Module(Test);

void Test::activity()
{
   EV << "This should not be printed: " << par("p1").doubleValue() << "\n";
}

}; //namespace

%exitcode: 1

%contains-regex: stderr
Cannot convert unit 's' \(second\) to 'w', at .*test\.ned:7

