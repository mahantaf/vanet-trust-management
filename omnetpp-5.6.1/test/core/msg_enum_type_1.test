%description:
Check enum usage in messages and classes

%file: test.msg

namespace @TESTNAME@;

enum MyEnum
{
    ECC = 1;
    PECC = 2;
    KIMEHECC = 5;
};

class MyClass
{
    MyEnum e1 = ECC;
    MyEnum e2 = PECC;
};

message MyMessage
{
    MyClass cc;
    MyEnum e1 = ECC;
    int e2 @enum(MyEnum) = PECC;
};


%includes:
#include "test_m.h"

%activity:

#define PRINT(X) EV << #X << ":" << X << endl

MyClass x;
MyMessage m;

PRINT(x.getE1());
PRINT(x.getE2());
PRINT(m.getCc().getE1());
PRINT(m.getCc().getE2());

%contains: stdout
x.getE1():1
x.getE2():2
m.getCc().getE1():1
m.getCc().getE2():2

%# four enum fields: three declared with enum type, one declared with @enum:
%contains-regex: test_m.cc
\s+if \(\!strcmp\(propertyname, *"enum"\)\) return "msg_enum_type_1::MyEnum";
.*
\s+if \(\!strcmp\(propertyname, *"enum"\)\) return "msg_enum_type_1::MyEnum";
.*
\s+if \(\!strcmp\(propertyname, *"enum"\)\) return "msg_enum_type_1::MyEnum";
.*
\s+if \(\!strcmp\(propertyname, *"enum"\)\) return "msg_enum_type_1::MyEnum";
