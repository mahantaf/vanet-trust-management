%description:
Check inheritance for messages

%file: test.msg

namespace @TESTNAME@;

message Base
{
    int b;
}

message MyMessage extends Base
{
    char c;
};

%includes:
#include "test_m.h"

%activity:

#define PRINT(X) EV << #X << ":" << X << endl

MyMessage x("msg");

EV << x.getClassName() << ":" << x.getName() << endl;
x.setB(11); PRINT(x.getB());
x.setC('x'); PRINT(x.getC());

%contains: stdout
MyMessage:msg
x.getB():11
x.getC():x

